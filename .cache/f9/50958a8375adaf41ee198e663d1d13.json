list-item {\n  align-items: center;\n  display: flex;\n  margin-bottom: 30px;\n\n  &:last-child {\n    .timeline-date::before {\n      display: none;\n    }\n  }\n}\n\n.timeline-list-item:last-child {\n  margin-bottom: 0;\n}\n\n.timeline-date {\n  align-items: center;\n  border: 2px solid white;\n  display: flex;\n  font-weight: bold;\n  font-family: \"Josefin Sans\", sans-serif;\n  font-size: 12px;\n  height: 22px;\n  justify-content: center;\n  letter-spacing: 1px;\n  margin-bottom: 0;\n  padding-top: 3px;\n  position: relative;\n  text-align: center;\n  text-transform: uppercase;\n  white-space: nowrap;\n  width: 75px;\n\n  &::before {\n    background: white;\n    content: \"\";\n    height: 42px;\n    position: absolute;\n    top: 27px;\n    width: 2px;\n  }\n}\n\n.timeline-event {\n  flex: 1;\n  margin: 8px 0 0 22px;\n}\n\n.timeline-event a {\n  color: inherit;\n  font-weight: 600;\n  text-decoration: none;\n}\n\n.timeline-event a:hover {\n  text-decoration: underline;\n}\n\n.link {\n  color: white;\n}\n\n@keyframes fadeIn {\n  from {\n    opacity: 0;\n  }\n\n  to {\n    opacity: 1;\n  }\n}\n\n@keyframes fadeInDown {\n  from {\n    opacity: 0;\n    transform: translateY(10%);\n  }\n\n  to {\n    opacity: 1;\n    transform: none;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"51135eec7f350610eeed5c0124242a3d","cacheData":{"env":{}}}